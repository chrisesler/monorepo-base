{
    "projectType": "library",
    "root": "libs/feature/shared/footer",
    "sourceRoot": "libs/feature/shared/footer/src",
    "prefix": "cesler",
    "targets": {
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["coverage/libs/feature/shared/footer"],
            "options": {
                "jestConfig": "libs/feature/shared/footer/jest.config.js",
                "passWithNoTests": true
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
                "lintFilePatterns": [
                    "libs/feature/shared/footer/src/**/*.ts",
                    "libs/feature/shared/footer/src/**/*.html"
                ]
            }
        }
    },
    "tags": ["scope:shared", "feature:shared"]
}
